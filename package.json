{
  "name": "version",
  "version": "0.0.1",
  "description": "Predictable file versioning",
  "main": "index.js",
  "scripts": {
    "test": "tape test/*.js",
    "start": "electron .",
    "build": "electron-packager . Version --platform=darwin --arch=x64 --version=0.36.7 --out=dist --overwrite",
    "zip": "zip -rX dist/Version-darwin-x64 dist/Version-darwin-x64/Version.app/",
    "dist": "npm run build && npm run zip",
    "clean": "rm -rf dist/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/callum/version.git"
  },
  "author": "Callum Jefferies",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/callum/version/issues"
  },
  "homepage": "https://github.com/callum/version#readme",
  "devDependencies": {
    "electron-packager": "^5.2.1",
    "electron-prebuilt": "^0.36.7",
    "tape": "^4.4.0"
  },
  "dependencies": {
    "version-filename": "github:callum/version-filename"
  }
}
